!URXVT
URxvt*termName:    rxvt
URxvt*scrollBar:   false
URxvt*urlLauncher: firefox
URxvt*buffered: true
!URxvt*font: xft:terminus:pixelsize=14
URxvt*font:           -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont:       -*-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
URxvt*italicFont:     -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt*boldItalicFont: -*-terminus-bold-r-*-*-12-*-*-*-*-*-*-*
URxvt*transparent: true
URxvt*shading: 100

!XTERM
xterm*toolBar: false
xterm*scrollBar: fasle
xterm*background: #1a1918
xterm*foreground: #a4a4a4
xterm*font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*

