					sakura -e ranger
				</execute>
			</action>
		</item>
		<separator/>
		<item label="Sakura">
			<action name="Execute">
				<execute>
					sakura
				</execute>
			</action>
		</item>
		<separator/>
		<menu id="5" label="Network">
			<item label="Shiretoko">
				<action name="Execute">
					<execute>
						firefox
					</execute>
				</action>
			</item>
