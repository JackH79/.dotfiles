			<item label="exit">
				<action name="Exit"/>
			</item>
			<separator/>
			<item label="reboot">
				<action name="Execute">
					<execute>
						sudo shutdown -r now
					</execute>
				</action>
			</item>
			<item label="shutdown">
				<action name="Execute">
					<execute>
						sudo shutdown -h now
					</execute>
				</action>
			</item>
		</menu>
