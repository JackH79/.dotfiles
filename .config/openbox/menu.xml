							xrandr --output LVDS --off
						</execute>
					</action>
				</item>
				<item label="LCD on">
					<action name="Execute">
						<execute>
							xrandr --output LVDS --auto
						</execute>
					</action>
				</item>
			</menu>
			<item label="htop">
				<action name="Execute">
					<execute>
						sakura -e htop
